#include <iostream>
using namespace std;
void swap(int *a,int x,int y){
    int temp=a[x];
    a[x]=a[y];
    a[y]=temp;
}
int main() {
    
    int a[10]={0,0,1,0,0,0,1,0,1,1};
    int i=0,j=9;
    while(i<=j){
        if(a[i]==0){
            i++;
        }
        else{
            if(a[j]!=0){
                j--;
            }
            else{
                swap(a,i,j);
                i++;j--;
            }
        }
    }
    for(i=0;i<10;i++){
        cout<<a[i]<<" "; 
    }
    return 0;
}



OR---------------------------------------------------------------------------------------------------------------


#include <iostream>
using namespace std;
void swap(int *a,int x,int y){
    int temp=a[x];
    a[x]=a[y];
    a[y]=temp;
}
int main() {
    
    int a[10]={0,0,1,0,0,0,1,0,1,1};
    int i=0,j=9;
    while(i<=j){
        if(a[i]==1){
            if(a[j]==0){
                swap(a,i,j);
            }
            j--;
        }
        else{
            i++;
        }
    }
    for(i=0;i<10;i++){
        cout<<a[i]<<" "; 
    }
    return 0;
}
