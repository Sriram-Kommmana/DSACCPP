#include <stdio.h>

int main() {
    int i,t,j,n=8;
    int a[8]={4,3,2,7,8,2,3,1};
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
            if(a[j]>a[j+1]){
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
            }
        }
    }

    i=0;
    while(i<n-1){
        if(a[i]==a[i+1]){
            printf("%d ",a[i]);
            i+=2;
        }
        else{
            i++;
        }
    }

    return 0;
}





OR---------------------------------------------------------------------------------->>>






#include <stdio.h>
int main() {
    int i,t,j,n=8;
    int a[8]={4,3,2,7,8,2,3,1};
    int b[8]={0};
    for(i=0;i<n;i++){
        b[a[i]]++;
    }
    for(i=0;i<n;i++){
        if(b[i]==2){
            printf("%d ",i);
        }
    }

    return 0;
}
