// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i,t,n=8;
    int a[8]={1,2,3,4,5,6,7,8};
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    }
    printf("\n");
    for(i=0;i<n;i+=2){
        if(i!=n-1){
            t=a[i];
            a[i]=a[i+1];
            a[i+1]=t;
        }   
        
    }
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    }
    return 0;
}
