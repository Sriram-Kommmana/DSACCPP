class Solution {
public:
    bool isVowel(char c){
        return c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U' || c == 'a' 
        || c == 'e' || c == 'i' || c == 'o' || c == 'u';
    }
    string sortVowels(string s) {
        unordered_map<char, int> count;

        for(char c: s){
            if(isVowel(c)) count[c]++;
        }
        string sortedVowel = "AEIOUaeiou";
        int j = 0;
        string ans;

        for(int i = 0; i < s.size(); i++){
            if(!isVowel(s[i])){
                ans += s[i];
            }
            else{
                while(count[sortedVowel[j]] == 0) j++;

                ans += sortedVowel[j];
                count[sortedVowel[j]]--;
            }
        }
        return ans;
    }
};
