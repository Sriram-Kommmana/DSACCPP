class Solution {
public:
    vector<int> largestDivisibleSubsetUsingLIS(vector<int>& nums){

        int n = nums.size();
        vector<int> dp(n, 1);
        vector<int> prevIndex(n, -1);

        int maxi = 1, maxIndex = 0;
        for(int i = 1; i < n; i++){
            for(int j = 0; j < i; j++){
                if((nums[i] % nums[j] == 0) && dp[j] + 1 > dp[i]){
                    dp[i] = dp[j] + 1;
                    prevIndex[i] = j;
                }
            }
            if(dp[i] > maxi){
                maxi = dp[i];
                maxIndex = i;
            }
        }

        vector<int> lisPrint;
        while(maxIndex != -1){
            lisPrint.push_back(nums[maxIndex]);
            maxIndex = prevIndex[maxIndex];
        }
        reverse(lisPrint.begin(), lisPrint.end());

        return lisPrint;
    }
    vector<int> largestDivisibleSubset(vector<int>& nums) {
        
        sort(nums.begin(), nums.end());
        return largestDivisibleSubsetUsingLIS(nums);

    }
};
